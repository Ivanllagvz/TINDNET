<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.TINDNET" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/fondo</item>
        <item name="colorPrimaryVariant">@color/fondo</item>
        <item name="colorOnPrimary">@color/fondo</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/fondo</item>
        <item name="colorSecondaryVariant">@color/fondo</item>
        <item name="colorOnSecondary">@color/fondo</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/fondo</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.TINDNET.NoActionBar">
        <item name="windowActionBar">true</item>
        <item name="windowNoTitle">true</item>
        <item name="android:statusBarColor">@color/fondo</item>
    </style>

    <style name="Theme.TINDNET.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.TINDNET.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>